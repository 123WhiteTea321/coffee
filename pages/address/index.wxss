.container{
    background-color:#f5f5f5 ;
    height: 500px;
    position: relative;
}
.add-address{
    position: absolute;
    bottom: 5px;
}
.blank{
    background-color: #f5f5f5;
    height: 15px;
}
.card{
    width: 90%;
    height: 70px;
    display: flex;
    background-color: white;
    justify-content: space-between;
    margin-left: 15px;
    /* margin-top: 10px; */
    border-radius: 15px;

}
.left{
    margin-left: 10px;

}
.name-phone-tip{
    padding-top: 12px;
    display: flex;
    font-size: 14px;
    justify-content: space-around;
}
.tip{
    background-color: blue;
    color: white;
    border-radius: 10px;
    font-size: 10px;
    width:25px;
    text-align: center;
    height: 15px;
}
.address{
    padding-top: 10px;
    font-size: 12px;
    padding-top: 8px;
    margin-left: 4px;

}
.img{
    width: 15px;
    margin-top: 20px;
    margin-right: 22px;
}
.img>image{
    width: 100%;
}

.add-address{
    background-color: blue;
    color: white;
    width: 90%;
    height: 27px;
    margin-left: 15px;
    text-align: center;
    line-height: 27px;
    border-radius: 20px;
    /* margin-top: 300px; */
}